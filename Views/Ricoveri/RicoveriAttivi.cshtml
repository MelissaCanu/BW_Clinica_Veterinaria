@model IEnumerable<BW_Clinica_Veterinaria.Models.Ricoveri>

@{
    ViewBag.Title = "RicoveriAttivi";
}
<h2>Ricoveri Attivi</h2>


@* Questa è la vista parziale che viene richiamata a seguito della chiamata AJAX*@
@foreach (var item in Model)
{
    <div>
        <h4>@Html.DisplayFor(modelItem => item.Animali.Nome)</h4>
        <p>@item.DataIN.ToString("dd-MM-yyyy")</p>
    </div>
}